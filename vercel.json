{
  "version": 2,
  "builds": [
    {
      "src": "html/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "css/index.css",
      "use": "@vercel/static"
    },
    {
      "src": "images/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/html/index.html"
    }
  ]
}